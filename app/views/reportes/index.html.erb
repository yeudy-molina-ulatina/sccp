<h1 class="content_title">Lista de reportes</h1>


    <table class="list_resourses">
  	<tr>
   	 <th bgcolor="white" scope="col">Temas discutidos</th>
   	 <th bgcolor="white" scope="col">Actividades realizadas</th>
   	 <th bgcolor="white" scope="col">Acuerdos</th>
   	 <th bgcolor="white" scope="col">Tipo</th>
   	 <th bgcolor="white" scope="col">Tratamiento</th>
   	 <th bgcolor="white" scope="col">Diagnostico</th>
   	 <th bgcolor="white" scope="col">Cita</th>
    
  	</tr>

<% @reportes.each do |reporte| %>
  <tr>
    <td><%= reporte.temas_discutidos %></td>
    <td><%= reporte.actividades_realizadas %></td>
    <td><%= reporte.acuerdos %></td>
    <td><%= reporte.tipo %></td>
    <td><%= reporte.tratamiento %></td>
    <td><%= reporte.diagnostico %></td>
    <td><%= reporte.cita %></td>
    <td><%= link_to 'Mostrar', reporte %></td>
    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Reporte', new_reporte_path %>
