<p id="notice">
	<%= notice %></p>

<p>
  <b>Fecha:</b>
  <%= @cita.fecha %>
</p>

<p>
  <b>Hora inicial:</b>
  <%= @cita.hora_inicio%>
</p>

<p>
  <b>Hora final:</b>
  <%= @cita.hora_fin%>
</p>

<p>
  <b>Numero consultorio:</b>
  <%= @cita.consultorio.numero_consultorio %>
</p>

<p>
  <b>Lugar atenci√≥n:</b>
  <%= @cita.lugar_atencion %>
</p>

<p>
  <b>Paciente(s):</b>
  <% @cita.pacientes.each do |paciente| %>
	<%= paciente.nombre_completo %><br/>	
  <% end %>
</p>

<p>
  <b>Tratante(s):</b>
  <% @cita.usuarios.each do |supervisor| %>
	<%= supervisor.nombre_completo %><br/>	
  <% end %>
</p>



