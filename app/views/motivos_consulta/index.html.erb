    <th></th>
  </tr>

<% @motivos_consulta.each do |motivo_consulta| %>
  <tr>
    <td><%= motivo_consulta.motivo_inicial %></td>
    <td><%= motivo_consulta.motivo_real %></td>
    <td><%= motivo_consulta.derivacion %></td>
    <td><%= motivo_consulta.estado %></td>
    <td><%= motivo_consulta.fecha_inicio %></td>
    <td><%= motivo_consulta.fecha_fin %></td>
